<%# <%= cl_image_tag("THE_IMAGE_ID_FROM_LIBRARY", %>
      <%# width: 400, height: 300, crop: :fill) %>
      <%= cl_image_tag @work_space.photo.key, height: 300, width: 400, crop: :fill %>

<h3>Work space Info</h3>

<p style="color: green"><%= notice %></p>

<%= render @work_space %>

<div>
  <%= link_to "Back to work spaces", work_spaces_path, class: "btn btn-secondary" %>
  <% if policy(@work_space).destroy? %>
    <%= link_to "Delete space", work_space_path(@work_space), data: { turbo_method: :delete } %>
  <% end %>
</div>

<div >
  <%= link_to "Booking", new_work_space_booking_path(@work_space), class: "btn btn-secondary" %>
</div>
