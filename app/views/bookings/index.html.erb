<h1>Bookings</h1>
<div>
  <ul>
    <% @bookings.each do |booking|%>
      <%= link_to booking do %>
        <li>
          <%= booking.start_date   %>
          <%= booking.end_date %>
          <%= booking.work_space.name %>
          <%= booking.amount_due %>
        </li>
      <%end%>
    <%end%>
  </ul>
  <div>
    <%= link_to "Create a new Booking", new_work_space_booking_path %>
  </div>
</div>
