<h1>Bookings</h1>

<ul>
  <% @bookings.each do |booking|%>
    <%= link_to booking do %>
      <li>
        <%= booking.start_date   %>
        <%= booking.end_date %>
        <%= booking.work_space.name %>
        <%= booking.amount_due %>
      </li>
    <%end%>
  <%end%>
</ul>

<div>
  <%= link_to "Create a new Booking", new_booking_path %>
</div>
